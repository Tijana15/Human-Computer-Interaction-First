<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="StartColorLighter" >#FF9AA8</Color>
    <Color x:Key="StartColorDarker">#FF6B8E</Color>


    <SolidColorBrush x:Key="SidebarBackground" Color="HotPink"/>
    <SolidColorBrush x:Key="MainContentBackground" Color="LightGray"/>
    <SolidColorBrush x:Key="TextColor" Color="LightPink"/>
    <SolidColorBrush x:Key="ButtonColor" Color="HotPink"/>
    <SolidColorBrush x:Key="DeleteButtonColor" Color="IndianRed"/>

    <SolidColorBrush x:Key="UserSideBarBackground" Color="DarkSeaGreen"/>
    <SolidColorBrush x:Key="UserMainBackground" Color="LightGray"/>
    <SolidColorBrush x:Key="UserTextColor" Color="White"/>
    <SolidColorBrush x:Key="UserUpdateButton" Color="YellowGreen"/>

</ResourceDictionary>